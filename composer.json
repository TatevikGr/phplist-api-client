{
    "name": "tatevikgr/rest-api-client",
    "description": "A PHP client for the phpList REST API",
    "type": "library",
    "authors": [
        {
            "name": "Tatevik Grigoryan",
            "email": "tatevik@phplist.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "symfony/http-foundation": "^6.0",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "squizlabs/php_codesniffer": "^3.2.0"
    },
    "autoload": {
        "psr-4": {
            "PhpList\\RestApiClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpList\\RestApiClient\\Tests\\": "tests/"
        }
    }
}
